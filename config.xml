<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="32045" id="com.transistorsoft.backgroundgeolocation.ionic2" version="4.13.4" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
